<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.hy.salon.basic.entity">
    <resultMap id="Job"  type="com.hy.salon.basic.entity.Job" extends="com.zhxh.core.data.TraceableEntity" >
        <result property="jobName" column="job_name"/>
        <result property="jobLevel" column="job_level"/>
    </resultMap>


    <resultMap id="Stuff" type="com.hy.salon.basic.entity.Stuff"  extends="com.zhxh.core.data.TraceableEntity">
        <result property="recordId" column="record_id"/>
        <result property="stuffName" column="stuff_name"/>
        <result property="tel" column="tel"/>
        <result property="gender" column="gender"/>
        <result property="entryTime" column="entry_time"/>
        <result property="workAge" column="work_age"/>
        <result property="special" column="special"/>
        <result property="dream" column="dream"/>
        <result property="weixin" column="weixin"/>
        <result property="qq" column="qq"/>
        <result property="address" column="address"/>
        <result property="createDate" column="create_date"/>
        <result property="createBy" column="create_by"/>
        <result property="updateDate" column="update_date"/>
        <result property="updateBy" column="update_by"/>
        <result property="optLock" column="opt_lock"/>
    </resultMap>



    <resultMap id="Salon" type="com.hy.salon.basic.entity.Salon" extends="com.zhxh.core.data.TraceableEntity">
        <result column="salon_name" property="salonName" jdbcType="VARCHAR" />
        <result column="parent_id" property="parentId" jdbcType="BIGINT" />
        <result column="tel" property="tel" jdbcType="VARCHAR" />
        <result column="city_id" property="cityId" jdbcType="BIGINT" />
        <result column="address" property="address" jdbcType="VARCHAR" />
        <result column="door_2_door" property="door2Door" jdbcType="BIT" />
        <result column="bed_num" property="bedNum" jdbcType="INTEGER" />
        <result column="area" property="area" jdbcType="DOUBLE" />
        <result column="time_open" property="timeOpen" jdbcType="TIME" />
        <result column="time_close" property="timeClose" jdbcType="TIME" />
        <result column="description" property="description" jdbcType="VARCHAR" />
    </resultMap>


    <resultMap id="StoreRoom" type="com.hy.salon.basic.entity.StoreRoom" extends="com.zhxh.core.data.TraceableEntity">
        <result column="store_id" property="storeId" jdbcType="BIGINT" />
        <result column="room_name" property="roomName" jdbcType="VARCHAR" />
        <result column="record_status" property="recordStatus" jdbcType="TINYINT" />

    </resultMap>

    <resultMap id="AttendanceSheet" type="com.hy.salon.basic.entity.AttendanceSheet" extends="com.zhxh.core.data.TraceableEntity">
        <result column="stuff_id" property="stuffId" jdbcType="BIGINT" />
        <result column="attendance_time" property="attendanceTime" jdbcType="TIMESTAMP" />
        <result column="address" property="address" jdbcType="VARCHAR" />
        <result column="create_date" property="createDate" jdbcType="TIMESTAMP" />
        <result column="create_by" property="createBy" jdbcType="BIGINT" />
        <result column="update_date" property="updateDate" jdbcType="TIMESTAMP" />
        <result column="update_by" property="updateBy" jdbcType="BIGINT" />
        <result column="opt_lock" property="optLock" jdbcType="INTEGER" />
    </resultMap>




</mapper>